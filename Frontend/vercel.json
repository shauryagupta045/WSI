{
    "version": 2,
    "name": "frontend",
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/images/(.*)",
        "dest": "/images/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "cleanUrls": true,
    "trailingSlash": false,
    "env": {
      "NODE_ENV": "production"
    }
  }